#include <TTSExtension.h>

TTSExtension PCF1;

void setup()
{
    Serial.begin(115200);
    PCF1.setValue(0);
}

void loop()
{
    for(int i = 0; i < 16; i++){
      uint16_t mask = 1 << i;
      PCF1.setValue(mask);
      delay(1000);
    }
}


